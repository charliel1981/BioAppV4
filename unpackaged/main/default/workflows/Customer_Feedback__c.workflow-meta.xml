<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>Email_general_feedback_to_genfeed_biocomposites_com</fullName>
        <ccEmails>genfeed@biocomposites.com</ccEmails>
        <description>Email general feedback to genfeed@biocomposites.com</description>
        <protected>false</protected>
        <senderType>CurrentUser</senderType>
        <template>unfiled$public/General_Feedback_Information</template>
    </alerts>
    <alerts>
        <fullName>Incident_Report_Information</fullName>
        <ccEmails>increp@biocomposites.com</ccEmails>
        <description>Email Incident Report Information</description>
        <protected>false</protected>
        <senderType>CurrentUser</senderType>
        <template>unfiled$public/Incident_Report_Information</template>
    </alerts>
    <rules>
        <fullName>Send feedback to genfeed%40biocomposites%2Ecom</fullName>
        <actions>
            <name>Email_general_feedback_to_genfeed_biocomposites_com</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Customer_Feedback__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>General Feedback</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Send feedback to increp%40biocomposites%2Ecom</fullName>
        <actions>
            <name>Incident_Report_Information</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Customer_Feedback__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Incident Report</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
