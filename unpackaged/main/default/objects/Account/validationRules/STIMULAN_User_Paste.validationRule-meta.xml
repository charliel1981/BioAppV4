<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>STIMULAN_User_Paste</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
STIMULAN_User__pc = FALSE,
STIMULAN_Paste__pc = TRUE)</errorConditionFormula>
    <errorDisplayField>STIMULAN_Paste__pc</errorDisplayField>
    <errorMessage>STIMULAN user needs to be selected before you can select STIMULAN paste</errorMessage>
</ValidationRule>
