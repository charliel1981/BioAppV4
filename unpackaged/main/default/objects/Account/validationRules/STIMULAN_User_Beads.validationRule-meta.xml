<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>STIMULAN_User_Beads</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
STIMULAN_User__pc = FALSE,
STIMULAN_Beads__pc = TRUE)</errorConditionFormula>
    <errorDisplayField>STIMULAN_Beads__pc</errorDisplayField>
